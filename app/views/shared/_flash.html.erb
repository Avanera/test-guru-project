<% flash.each do |key, message| %>
<div class="<%= flash_class(key) %>" role="alert">
  <%= message.html_safe %>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
  <!-- content_tag :p, message, class: "flash #{key}" -->
<% end %>
