<tr>
  <td class="align-middle">
    <%= test.title %>
    <%= render 'form_inline', test: test == @test ? @test : test %>
  </td>
  <td class="align-middle"><%= test_level(test) %></td>
  <td class="align-middle"><%= test.questions.count %></td>
  <td class="align-middle"><%= link_to t('shared.show'), admin_test_path(test) %></td>
  <td class="align-middle"><%= link_to t('shared.edit'), edit_admin_test_path(test) %></td>
  <td class="align-middle">
    <%= link_to t('shared.delete'),
                admin_test_path(test),
                method: :delete,
                data: { confirm: t('shared.confirm')} %>
  </td>
</tr>
