<h1>Listing tests</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Level</th>
    <th></th>
  </tr>

  <% @tests.each do |test| %>
    <tr>
      <td><%= test.title %></td>
      <td><%= test.level %></td>
      <td><%= link_to 'Show', test_path(test) %></td>
      <td><%= link_to 'Edit', edit_test_path(test) %></td>
    </tr>
  <% end %>
</table>
